        "@semantic-release/npm": "~9.0.1",
        "@typescript-eslint/parser": "~5.21.0",
        "@typescript-eslint/eslint-plugin": "~5.21.0",
        "@types/chai-as-promised": "~7.1.5",
        "@types/node": "~18.6.2",
        "typescript": "~4.7.2",
        "prettier": "2.6.1",
        "mocha": "~9.2.1",
        "chai": "~4.3.6",
        "c8": "~7.11.0",
        "eslint": "~8.14.0",
        "eslint-plugin-import": "~2.26.0",
        "eslint-config-prettier": "~8.5.0",
