{
    "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
    "rushVersion": "5.62.4",
    "pnpmVersion": "6.7.1",
    "pnpmOptions": {
        "useWorkspaces": true
    },
    "nodeSupportedVersionRange": ">=16.13.0 <18.0.0 || >=18.0.0 <19.0.0",
    "gitPolicy": {},
    "repository": {
        "url": "https://github.com/Julien-Pires/Kocoa",
        "defaultBranch": "main",
        "defaultRemote": "origin"
    },
    "eventHooks": {
        "preRushInstall": [],
        "postRushInstall": [],
        "preRushBuild": [],
        "postRushBuild": []
    },
    "variants": [],
    "projectFolderMaxDepth": 3,
    "projects": [
        {
            "packageName": "kocoa",
            "projectFolder": "packages/kocoa"
        },
        {
            "packageName": "kocoa-tutorial",
            "projectFolder": "samples/kocoa-tutorial"
        },
        {
            "packageName": "@kocoa/core",
            "projectFolder": "packages/kocoa-core"
        },
        {
            "packageName": "@kocoa/adapter-mocha",
            "projectFolder": "packages/kocoa-adapter-mocha"
        },
        {
            "packageName": "@toolbox/scripts",
            "projectFolder": "toolbox/scripts"
        },
        {
            "packageName": "semantic-release-rush-dependency",
            "projectFolder": "packages/semantic-release-rush-dependency"
        }
    ]
}